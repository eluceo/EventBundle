{
    "name":"eluceo/event-bundle",
    "description":"EventBundle for Symfony 2 and Sonata Admin. This bundles offers an event-calendar. You can create an event with several dates, upload an event-image and you will be able to upload a picture-gallery for an event.",
    "homepage":"https://github.com/eluceo/EventBundle",
    "authors":[
        {
            "name":"Markus Poerschke",
            "email":"markus@eluceo.de",
            "role":"Developer"
        }
    ],
    "autoload":{
        "psr-0":{
            "Eluceo\\EventBundle":""
        }
    },
    "target-dir":"Eluceo/EventBundle",
    "require":{
        "php":">=5.4.0",
        "sonata-project/admin-bundle":"2.2.*",
        "sonata-project/doctrine-orm-admin-bundle":"*",
        "sonata-project/seo-bundle":"*",
        "knplabs/knp-paginator-bundle":"*",
        "doctrine/doctrine-fixtures-bundle":"*"
    }
}
